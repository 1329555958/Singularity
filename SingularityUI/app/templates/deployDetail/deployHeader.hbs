{{! Part of deployBase }}
<header class='detail-header'>
    <div class="row">
        <div class="col-md-8">
            <h5>
                <span>
                    Request <a href="{{appRoot}}/request/{{ data.requestId }}">{{ data.requestId }}</a>
                </span>
            </h5>
        </div>
    </div>

    <div class="row">
        {{! Left side, info }}
        <div class="col-md-8">
            <h3>
                <span>
                    {{ data.deployId }}
                </span>
            </h3>
        </div>

        {{! Right side, buttons }}
        <div class="col-md-4 button-container">
            <a class="btn btn-default" data-request-id="{{ data.id }}" data-action="viewObjectJSON">
                JSON
            </a>
        </div>
    </div>
</header>
