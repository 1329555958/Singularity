<header class="jumbotron subhead" id="overview">
    <h3><a href="{{#appRoot}}{{/appRoot}}/request/{{ taskHistory.task.taskId.requestId }}">{{#hardBreak taskHistory.task.taskId.requestId}}{{/hardBreak}}</a></h3>
    <h1><a href="{{#appRoot}}{{/appRoot}}/task/{{ taskHistory.task.id }}">{{#hardBreak taskHistory.task.id}}{{/hardBreak}}</a></h1>
</header>

<section>
    <ol class="breadcrumb">
        {{#eachWithFn breadcrumbs}}
            {{#if _last}}
                <li class="active">{{ path }}</li>
            {{else}}
                <li><a href="{{#appRoot}}{{/appRoot}}/{{ pathRoute }}">{{ path }}</a></li>
            {{/if}}
        {{/eachWithFn}}
    </ol>

    <div class="row-fluid">
        <div class="span12">
            {{> filesTable}}
        </div>
    </div>
</section>