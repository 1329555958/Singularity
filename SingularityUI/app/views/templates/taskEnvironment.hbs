{{#if taskHistory.task.mesosTask.executor.command.environment.variables}}
<section data-task-environment>
    <div class="page-header">
        <h1>Environment Variables</h1>
    </div>
    <div class="row-fluid">
        <div class="span12">
            <dl class="wider">
                {{#taskHistory.task.mesosTask.executor.command.environment.variables}}
                    <dt>{{ name }}</dt>
                    <dd><pre>{{ value }}</pre></dd>
                {{/taskHistory.task.mesosTask.executor.command.environment.variables}}
            </dl>
        </div>
    </div>
</section>
{{/if}}