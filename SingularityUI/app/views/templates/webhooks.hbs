<header class="jumbotron subhead" id="overview">
    <h1>Webhooks</h1>
</header>

<section>
    <div class="row-fluid">
        <div class="span12">
            <table class="table" data-sortable="true">
                <thead>
                    <tr>
                        <th>Webhook</th>
                    </tr>
                </thead>
                <tbody>
                    {{#eachWithFn webhooks}}
                        <tr>
                            <td><a target="_blank" href="{{ url }}">{{#hardBreak url}}{{/hardBreak}}</a></td>
                        </tr>
                    {{/eachWithFn}}
                </tbody>
            </table>
        </div>
    </div>
</section>